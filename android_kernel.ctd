    return retval;
}
    这段代码为出错处理; 
    struct kset *kset_create_and_add(const char *name,  struct kset_uevent_ops *uevent_ops, 
                                                             struct kobject *parent_kobj){     
        struct kset *kset;     
        int error;
             
     //创建一个kset     
        kset = kset_create(name, uevent_ops, parent_kobj);     
        if (!kset)         
            return NULL;
                 
      //注册kset     
        error = kset_register(kset);     
        if (error) {        
         //如果注册失败,释放kset         
            kfree(kset);         
