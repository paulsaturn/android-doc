static void late_resume(struct work_struct *work)
{
       struct early_suspend *pos;
       unsigned long irqflags;
       int abort = 0;
    int completed = 0;
 
       mutex_lock(&amp;early_suspend_lock);
       spin_lock_irqsave(&amp;state_lock, irqflags);
 
