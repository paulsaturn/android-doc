46.     // 使能非启动cpu  
47.     enable_nonboot_cpus();  
48.   
49.  Platform_wake:  
50.     // 处理器开始唤醒  
51.     if (suspend_ops-&gt;wake)  
52.         suspend_ops-&gt;wake();  
53.   
