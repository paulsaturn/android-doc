       if (state == SUSPENDED)
              state &amp;= ~SUSPENDED;    </rich_text><rich_text foreground="#969696">// state = 0</rich_text><rich_text>
       else
              abort = 1;
       spin_unlock_irqrestore(&amp;state_lock, irqflags);
 
       if (abort) {
              if (Earlysuspend_debug_mask &amp; DEBUG_SUSPEND)
