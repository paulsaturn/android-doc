24.         scllo(adap);  
25.     }  
26.     sdahi(adap);  
27.     if (sclhi(adap) &lt; 0) { /* timeout */  
28.         bit_dbg(1, &amp;i2c_adap-&gt;dev, &quot;i2c_outb: 0x%02x, &quot;  
29.             &quot;timeout at ack\n&quot;, (int)c);  
