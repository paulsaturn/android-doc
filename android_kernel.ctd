1. static void setup_memory_tags (bd_t *bd)  
2. {  
3.     int i;  
4.   
5.     for (i = 0; i &lt; CONFIG_NR_DRAM_BANKS; i++) {  
6.         params-&gt;hdr.tag = ATAG_MEM;  
