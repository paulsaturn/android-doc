4. #define SDP_ERROR_RSP       0x01  
5. #define SDP_SVC_SEARCH_REQ  0x02  
6. #define SDP_SVC_SEARCH_RSP  0x03  
7. #define SDP_SVC_ATTR_REQ    0x04  
8. #define SDP_SVC_ATTR_RSP    0x05  
9. #define SDP_SVC_SEARCH_ATTR_REQ 0x06  
