	    setup_serial_tag (&amp;params);
    #endif
    #ifdef CONFIG_REVISION_TAG
	    setup_revision_tag (&amp;params);
    #endif
    #ifdef CONFIG_SETUP_MEMORY_TAGS
	    setup_memory_tags (bd);
    #endif
    #ifdef CONFIG_CMDLINE_TAG
	    if(strlen((const char *)hdr-&gt;cmdline)) {
